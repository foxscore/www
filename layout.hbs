<!DOCUMENT html>
<html lang="en">
<head>
    <title>Fox_score</title>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <link rel="stylesheet" href="/style.css"/>
    <link rel="icon" type="image/png" href="/rext_center.png"/>
    <link rel="stylesheet" href="https://www.nerdfonts.com/assets/css/webfont.css">
    <link rel="stylesheet" href="https://unpkg.com/tippy.js@6.3.7/animations/shift-away.css">
    <script src="https://unpkg.com/@popperjs/core@2" defer></script>
    <script src="https://unpkg.com/tippy.js@6" defer></script>
    <script>let exports = {}</script>
    <!--suppress HtmlUnknownTarget -->
    <script src="/js/core.js"></script>
    <!--suppress HtmlUnknownTarget -->
    <script src="/js/notifications.js"></script>
    <!--suppress HtmlUnknownTarget -->
    <script src="/js/random.js"></script>
    <!--suppress HtmlUnknownTarget -->
    <script src="/js/tooltips.js"></script>
</head>
<body>

<nav id="nav">
    <start>
        <a href="/">
            <button
                    {{#ifEquals tab "home"}}selected{{/ifEquals}}
                    tooltip="Home" tooltip-placement="right" tooltip-variant="nav-item">
                <i class="nf nf-fa-home"></i>
                <span>Home</span>
            </button>
        </a>
        <a href="/projects">
            <button
                    {{#ifEquals tab "projects"}}selected{{/ifEquals}}
                    tooltip="Projects" tooltip-placement="right" tooltip-variant="nav-item">
                <i class="nf nf-fa-code"></i>
                <span>Projects</span>
            </button>
        </a>
        <a href="/vpm">
            <button
                    {{#ifEquals tab "vpm"}}selected{{/ifEquals}}
                    tooltip="VPM Repository" tooltip-placement="right" tooltip-variant="nav-item">
                <i class="nf nf-md-package_variant_closed"></i>
                <span>VPM Repository</span>
            </button>
        </a>
    </start>

    <end>
        <button invisible id="nav-toggle" tooltip="Expand Menu" tooltip-placement="right" tooltip-variant="nav-item" onclick="Core.ToggleNavigation()">
            <i class="nf nf-md-menu_right"></i>
            <span>Collapse</span>
        </button>
    </end>
</nav>

<main id="main">

    <h1 class="mb-0">Fox_score</h1>
    <div class="mb-2">
            <span text-neutral relative style="top: -0.5rem;">
                Austrian Software Developer
            </span>
    </div>

    {{> @partial-block }}

</main>

<script src="js/tooltips.js"></script>

</body>
</html>